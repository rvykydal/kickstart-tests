#test name: network-noipv4-httpks
url @KSTEST_URL@
install

network --device=enp0s4 --noipv4 --no-activate --onboot=yes
network --device=enp0s5 --noipv4 --activate --onboot=no

bootloader --timeout=1
zerombr
clearpart --all
autopart

keyboard us
lang en
timezone America/New_York
rootpw qweqwe
shutdown

%packages
%end

%post --nochroot

@KSINCLUDE@ post-nochroot-lib-network.sh

check_gui_configurations enp0s3 enp0s4 enp0s5

%end

%post

@KSINCLUDE@ post-lib-network.sh

if [[ "@KSTEST_OS_NAME@" == "rhel" ]]; then
    check_device_ifcfg_value enp0s4 BOOTPROTO none
    # BOOTPROTO is removed from the ifcfg when real ONBOOT value is updated via NM api
    check_ifcfg_key_exists enp0s5 BOOTPROTO no
else
    check_ifcfg_key_exists enp0s4 BOOTPROTO no
    check_ifcfg_key_exists enp0s5 BOOTPROTO no
fi

# TODO create test with virtual network having ipv6 enabled (ipv6 only connection)
#      libvirt default net does not have ipv6 enabled
check_device_connected enp0s5 no
check_device_has_ipv4_address enp0s5 no

# No error was written to /root/RESULT file, everything is OK
if [[ ! -e /root/RESULT ]]; then
   echo SUCCESS > /root/RESULT
fi
%end
